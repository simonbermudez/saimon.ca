{
  "name": "saimon",
  "version": "1.0.0",
  "description": "AI/ML Labs showcase website featuring GPU cluster and open-source stack",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "docker:build": "docker build -t saimon-app .",
    "docker:run": "docker run -d -p 3000:80 --name saimon-container saimon-app",
    "docker:stop": "docker stop saimon-container && docker rm saimon-container",
    "docker:logs": "docker logs saimon-container",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "three": "^0.160.0",
    "gsap": "^3.12.2"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32"
  },
  "keywords": ["ai", "ml", "gpu", "cluster", "three.js", "webgl"],
  "author": "Simon Bermudez",
  "license": "MIT"
}
